{
  "name": "Test-Laptop",
  "override_attributes": {
    "bcpc": {
      "virt_type" : "qemu",
      "ceph": {
        "hdd_disks" : [ "sdb", "sdc" ],
        "ssd_disks" : [ "sdd", "sde" ]
      },
      "domain_name" : "bcpc.example.com",
      "management": {
        "vip" : "10.0.100.5",
        "interface" : "eth0",
        "netmask" : "255.255.240.0",
        "cidr" : "172.31.32.0/20",
        "gateway" : "172.31.32.1"
      },
      "storage": {
        "interface" : "eth1",
        "netmask" : "255.255.255.0",
        "cidr" : "172.16.100.0/24",
        "gateway" : "172.16.100.3"
      },
      "floating": {
        "vip" : "192.168.100.5",
        "interface" : "eth0",
        "netmask" : "255.255.255.0",
        "cidr" : "192.168.100.0/24",
        "gateway" : "192.168.100.3",
        "available_subnet" : "192.168.100.128/25"
      },
      "bootstrap": {
        "interface" : "eth0",
        "pxe_interface" : "eth1",
        "server" : "172.31.42.87",
        "dhcp_subnet" : "172.31.32.0",
        "dhcp_range" : "172.31.42.87"
      },
      "dns_servers" : ["172.31.0.2"],
      "hadoop" : {
        "min_node_count" : 2,
        "disks" : ["xvdb"],
        "distribution" : { 
          "version" : "-cdh5",
          "key" : "cloudera-archive-5.key"
        },
        "hdfs" : {
          "failed_volumes_tolerated" : 0
        }
      },
      "repos" : {
        "cloudera" : "http://archive.cloudera.com/cdh5/ubuntu/precise/amd64/cdh"
      }
    },
    "ntp": {
       "servers" : [ "0.pool.ntp.org", "1.pool.ntp.org", "2.pool.ntp.org", "3.pool.ntp.org" ]
    },
    "chef_client": {
      "server_url": "http://10.0.100.3:4000",
      "cache_path": "/var/chef/cache",
      "backup_path": "/var/chef/backup",
      "validation_client_name": "chef-validator",
      "run_path": "/var/chef"
    }
  },
  "json_class": "Chef::Environment",
  "description": "Laptop test environment",
  "cookbook_versions": {
  },
  "chef_type": "environment"
}
